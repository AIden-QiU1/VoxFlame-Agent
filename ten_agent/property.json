{
  "ten": {
    "predefined_graphs": [
      {
        "name": "voxflame_voice_assistant",
        "auto_start": true,
        "graph": {
          "nodes": [
            {
              "type": "extension",
              "name": "websocket_server",
              "addon": "websocket_server",
              "extension_group": "default",
              "property": {
                "port": 8765,
                "host": "0.0.0.0",
                "sample_rate": 16000,
                "channels": 1,
                "bytes_per_sample": 2
              }
            },
            {
              "type": "extension",
              "name": "stt",
              "addon": "aliyun_asr_bigmodel_python",
              "extension_group": "stt",
              "property": {
                "params": {
                  "api_key": "${env:DASHSCOPE_API_KEY}",
                  "model": "paraformer-realtime-v2",
                  "language_hints": ["zh", "en"],
                  "sample_rate": 16000
                }
              }
            },
            {
              "type": "extension",
              "name": "llm",
              "addon": "openai_llm2_python",
              "extension_group": "chatgpt",
              "property": {
                "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1",
                "api_key": "${env:DASHSCOPE_API_KEY}",
                "frequency_penalty": 0.9,
                "model": "${env:DASHSCOPE_LLM_MODEL|qwen3-max}",
                "max_tokens": 512,
                "prompt": "你是VoxFlame燃言助手，专门帮助有构音障碍的老年人进行日常对话。请用简洁、清晰、友好的语言回复。",
                "proxy_url": "",
                "greeting": "您好！我是燃言助手，有什么可以帮助您的？",
                "max_memory_length": 10
              }
            },
            {
              "type": "extension",
              "name": "tts",
              "addon": "cosy_tts_python",
              "extension_group": "tts",
              "property": {
                "params": {
                  "api_key": "${env:DASHSCOPE_API_KEY}",
                  "model": "cosyvoice-v3",
                  "sample_rate": 16000,
                  "voice": "longxiaochun"
                }
              }
            }
          ],
          "connections": [
            {
              "extension": "websocket_server",
              "audio_frame": [
                {
                  "name": "pcm_frame",
                  "dest": [
                    {
                      "extension": "stt"
                    }
                  ]
                },
                {
                  "name": "pcm_frame",
                  "source": [
                    {
                      "extension": "tts"
                    }
                  ]
                }
              ]
            },
            {
              "extension": "llm",
              "data": [
                {
                  "name": "asr_result",
                  "source": [
                    {
                      "extension": "stt"
                    }
                  ]
                }
              ]
            },
            {
              "extension": "tts",
              "data": [
                {
                  "name": "text_data",
                  "source": [
                    {
                      "extension": "llm"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    ],
    "log": {
      "handlers": [
        {
          "matchers": [
            {
              "level": "info"
            }
          ],
          "formatter": {
            "type": "plain",
            "colored": true
          },
          "emitter": {
            "type": "console",
            "config": {
              "stream": "stdout"
            }
          }
        }
      ]
    }
  }
}
