{
  "type": "extension",
  "name": "dashscope_asr_python",
  "version": "1.0.0",
  "language": "python",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.4"
    }
  ],
  "api": {
    "property": {
      "api_key": {
        "type": "string"
      },
      "model": {
        "type": "string",
        "default": "paraformer-realtime-v2"
      },
      "sample_rate": {
        "type": "int32",
        "default": 16000
      },
      "format": {
        "type": "string",
        "default": "pcm"
      }
    },
    "data_in": [
      {
        "name": "pcm_frame",
        "property": {
          "sample_rate": {"type": "int32"},
          "samples_per_channel": {"type": "int32"},
          "bytes_per_sample": {"type": "int32"},
          "number_of_channels": {"type": "int32"},
          "timestamp": {"type": "int64"},
          "data": {"type": "buf"}
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {"type": "string"},
          "is_final": {"type": "bool"},
          "confidence": {"type": "float64"},
          "stream_id": {"type": "uint32"}
        }
      }
    ]
  }
}
