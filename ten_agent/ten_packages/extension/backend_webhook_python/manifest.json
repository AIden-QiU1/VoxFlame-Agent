{
  "type": "extension",
  "name": "backend_webhook_python",
  "version": "0.1.0",
  "language": "python",
  "dependencies": [],
  "api": {
    "property": {
      "webhook_url": {
        "type": "string"
      },
      "user_id": {
        "type": "string"
      },
      "session_id": {
        "type": "string"
      },
      "enable_logging": {
        "type": "bool"
      },
      "timeout": {
        "type": "int32"
      }
    },
    "data_in": [
      {
        "name": "text_data",
        "property": {
          "text": {
            "type": "string"
          },
          "is_final": {
            "type": "bool"
          },
          "source": {
            "type": "string"
          }
        }
      },
      {
        "name": "tool_result",
        "property": {
          "tool_name": {
            "type": "string"
          },
          "result": {
            "type": "string"
          },
          "success": {
            "type": "bool"
          }
        }
      }
    ],
    "data_out": [
      {
        "name": "user_context",
        "property": {
          "user_profile": {
            "type": "string"
          },
          "preferences": {
            "type": "string"
          },
          "history": {
            "type": "string"
          }
        }
      }
    ],
    "cmd_in": [
      {
        "name": "get_user_profile"
      },
      {
        "name": "execute_tool"
      }
    ],
    "cmd_out": [
      {
        "name": "tool_execute"
      }
    ]
  }
}
