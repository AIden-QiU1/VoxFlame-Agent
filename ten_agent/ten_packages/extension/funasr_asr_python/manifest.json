{
  "type": "extension",
  "name": "funasr_asr_python",
  "version": "1.0.0",
  "language": "python",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.4"
    }
  ],
  "api": {
    "property": {
      "model": {
        "type": "string",
        "default": "sensevoice-small"
      },
      "device": {
        "type": "string", 
        "default": "cuda:0"
      },
      "language": {
        "type": "string",
        "default": "auto"
      },
      "hotwords": {
        "type": "array",
        "items": {"type": "string"},
        "default": []
      },
      "vad_model": {
        "type": "string",
        "default": "fsmn-vad"
      },
      "punc_model": {
        "type": "string",
        "default": "ct-punc"
      }
    },
    "data_in": [
      {
        "name": "pcm_frame",
        "property": {
          "sample_rate": {"type": "int32"},
          "samples_per_channel": {"type": "int32"},
          "bytes_per_sample": {"type": "int32"},
          "number_of_channels": {"type": "int32"},
          "timestamp": {"type": "int64"},
          "data": {"type": "buf"}
        }
      }
    ],
    "data_out": [
      {
        "name": "text_data",
        "property": {
          "text": {"type": "string"},
          "is_final": {"type": "bool"},
          "confidence": {"type": "float64"},
          "stream_id": {"type": "uint32"}
        }
      }
    ]
  }
}
