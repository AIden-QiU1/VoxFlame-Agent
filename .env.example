# Backend API Configuration
BACKEND_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# TEN Agent Configuration
TEN_AGENT_URL=http://localhost:8080

# DashScope API Configuration (阿里云灵积)
DASHSCOPE_API_KEY=your_dashscope_api_key

# QWEN3 LLM Model Configuration
# 可选值: qwen3-max (旗舰), qwen-plus (平衡), qwen-turbo (快速)
DASHSCOPE_LLM_MODEL=qwen3-max

# DashScope ASR Configuration
DASHSCOPE_ASR_MODEL=paraformer-realtime-v2

# DashScope TTS Configuration
DASHSCOPE_TTS_MODEL=cosyvoice-v1

# PowerMem Configuration
POWERMEM_ENABLED=true
POWERMEM_DB_PATH=./storage/powermem.db
FAISS_INDEX_PATH=./data/powermem.faiss_index
EMBEDDING_DIMENSION=384

# Server Ports
BACKEND_PORT=3001
FRONTEND_PORT=3000
TEN_AGENT_PORT=8080
WEBSOCKET_PORT=8765

# Development Mode
NODE_ENV=development
